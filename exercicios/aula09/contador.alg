algoritmo "contageminteligente"
var
ini,fim,conP,conR: Inteiro
inicio
Escreva ("Inicio: ")
Leia (ini)
Escreva ("Fim: ")
Leia (fim)
conP <- ini
conR <- ini
Enquanto (conP <= fim) faca
   Escreva (conP)
   conP <- conP + 1
FimEnquanto
Enquanto (conR >= fim) faca
   Escreva (conR)
   conR <- conR - 1
FimEnquanto
fimalgoritmo
